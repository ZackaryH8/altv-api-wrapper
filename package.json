{
  "name": "altv-api-wrapper",
  "version": "1.1.0",
  "description": "NodeJS Wrapper for the alt:V masterlist API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "prepublish": "tsc",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "build-docs": "typedoc --options",
    "watch": "tsc-watch --onSuccess \"node dist/index.js\"",
    "prettier": "prettier --write ./**/*.ts"
  },
  "keywords": [
    "altv",
    "api",
    "wrapper",
    "typescript"
  ],
  "author": "zackaryh8",
  "license": "ISC",
  "devDependencies": {
    "@types/node-fetch": "^2.5.7",
    "@types/xml2js": "^0.4.8",
    "prettier": "^2.3.0",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "typedoc": "^0.20.14",
    "typedoc-theme-dark": "0.0.5",
    "typescript": "^4.1.3"
  },
  "directories": {
    "lib": "lib"
  },
  "exclude": [
    "./tests/",
    "./node_modules/",
    "./dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZackaryH8/altv-api-wrapper.git"
  },
  "bugs": {
    "url": "https://github.com/ZackaryH8/altv-api-wrapper/issues"
  },
  "homepage": "https://github.com/ZackaryH8/altv-api-wrapper#readme",
  "dependencies": {
    "axios": "^0.21.1"
  }
}
